# Postgres container settings
POSTGRES_USER=rsruser
POSTGRES_PASSWORD=rsrpass
POSTGRES_DB=readysetrentables
POSTGRES_HOST_PORT=5432

# API container
ASPNETCORE_ENVIRONMENT=Development
API_HOST_PORT=5100

# Connection strings consumed by ASP.NET config binder
CONNECTIONSTRINGS__POSTGRESQL=Host=postgres;Port=5432;Database=readysetrentables;Username=rsruser;Password=rsrpass

# Frontend - set this per machine
PUBLIC_API_BASE_URL=http://192.168.1.3:5100
FRONTEND_HOST_PORT=3001