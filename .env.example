# Postgres container settings
POSTGRES_USER=rsruser
POSTGRES_PASSWORD=CHANGE_ME
POSTGRES_DB=readysetrentables
POSTGRES_HOST_PORT=5433

# API container
ASPNETCORE_ENVIRONMENT=Development
API_HOST_PORT=5100

# Connection strings consumed by ASP.NET config binder
CONNECTIONSTRINGS__POSTGRESQL=Host=postgres;Port=5432;Database=readysetrentables;Username=rsruser;Password=CHANGE_ME
CONNECTIONSTRINGS__DEFAULT=Host=postgres;Port=5432;Database=readysetrentables;Username=rsruser;Password=CHANGE_ME

# LanceDB
LANCEDB_HOST_PORT=8001
LANCEDB_API_BASEURL=http://lancedb-api:8000
EMBEDDING_MODEL=all-MiniLM-L6-v2
LANCEDB_LOG_LEVEL=INFO

# Frontend - set this per machine
PUBLIC_API_BASE_URL=http://192.168.1.3:5100
FRONTEND_HOST_PORT=3001
